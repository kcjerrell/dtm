{
  "name": "dtm",
  "author": "kcjerrell",
  "private": true,
  "version": "0.2.1",
  "type": "module",
  "scripts": {
    "dev": "tauri dev",
    "dev:web": "MOCK_TAURI=true vite",
    "dev:vite": "vite",
    "build": "vite build",
    "build:mac": "dotenv -- tauri build",
    "build:universal": "dotenv -- tauri build --target universal-apple-darwin",
    "clean:vite": "rm --recursive node_modules/.vite & rm --recursive  node_modules/.vite-temp",
    "preview": "vite preview",
    "tauri": "dotenv -- tauri",
    "gen-icons": "tauri icon ./app-icon.png",
    "generate:entities": "dotenv -e .env -- sh -c 'sea-orm-cli generate entity --database-url \"$DB_URL\" --ignore-tables images_fts,images_fts_data,images_fts_docsize,images_fts_idx,images_fts_config,images_fts_content --output-dir src-tauri/entity/src --with-serde serialize --entity-format dense'",
    "version": "tsx ./scripts/version.ts"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.30.0",
    "@emotion/react": "^11.14.0",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-clipboard-manager": "~2",
    "@tauri-apps/plugin-dialog": "~2",
    "@tauri-apps/plugin-fs": "~2",
    "@tauri-apps/plugin-http": "~2",
    "@tauri-apps/plugin-log": "^2.8.0",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-process": "~2",
    "@tauri-apps/plugin-shell": "~2",
    "@tauri-apps/plugin-store": "~2",
    "@tauri-apps/plugin-updater": "~2",
    "@tauri-apps/plugin-window-state": "~2",
    "@tauri-store/valtio": "^3.2.0",
    "@uiw/react-json-view": "^2.0.0-alpha.40",
    "async-mutex": "^0.5.0",
    "derive-valtio": "^0.2.0",
    "eventemitter3": "^5.0.1",
    "exifr": "github:m-hayabusa/exifr#2cf6c14fcd8eab52a7a7de53af7fdb01a4a9f90f",
    "exifreader": "^4.35.0",
    "motion": "^12.25.0",
    "nanoid": "^5.1.6",
    "next-themes": "^0.4.6",
    "plist": "^3.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^6.0.3",
    "react-hotkeys-hook": "^5.2.1",
    "react-icons": "^5.5.0",
    "tauri-plugin-clipboard-api": "^2.1.11",
    "tauri-plugin-fs-watch-api": "https://github.com/tauri-apps/tauri-plugin-fs-watch#v1",
    "tauri-plugin-nspopover": "git+https://github.com/freethinkel/tauri-nspopover-plugin",
    "valtio": "^2.3.0",
    "valtio-reactive": "^0.1.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@biomejs/biome": "2.3.11",
    "@octokit/rest": "^22.0.1",
    "@tauri-apps/cli": "^2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/fs-extra": "^11.0.4",
    "@types/plist": "^3.0.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "fs-extra": "^11.3.3",
